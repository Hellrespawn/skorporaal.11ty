{
  "name": "skorporaal.11ty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:tailwind": "del-cli src/static/styles.min.css",
    "clean:11ty": "del-cli _site",
    "dev:postcss": "postcss src/_assets/**/*.css -o src/static/styles.min.css --watch --verbose",
    "dev:11ty": "eleventy --serve --watch",
    "build:postcss": "NODE_ENV=production postcss src/_assets/**/*.css -o src/static/styles.min.css",
    "build:11ty": "ELEVENTY_ENV=production eleventy",
    "format:prettier": "prettier --write ./src/**/*",
    "clean": "npm-run-all --parallel clean:*",
    "dev": "npm-run-all --parallel dev:*",
    "dev-clean": "npm-run-all clean build:* --parallel dev:*",
    "build": "npm-run-all clean build:*",
    "format": "npm-run-all format:*",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.8",
    "del-cli": "^4.0.1",
    "markdown-it": "^13.0.1",
    "markdown-it-external-links": "^0.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24"
  }
}
